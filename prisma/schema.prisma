generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["jsonProtocol"]
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Portfolio {
  id Int @id @default(autoincrement())
  title String
  description String
  url String?
  repoUrl String?
  image String
  categories String[]
  date String
}
